	keywords = {mine, Bibliographies/Books, DLS, Rarefaction, article},
	pages = {3412--3420},
	file = {Attachment:/Users/smitdave/Zotero/storage/8MS94GFJ/Singh et al. 2003 - 5C9704F7-689D-40AC-A510-F0C78F49DA9F.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/GZ4834BY/Singh et al. 2003 - Microbial diversity of biofilms in dental unit water systems.pdf:application/pdf;Singh et al. - 2003 - Microbial diversity of biofilms in dental unit wat.pdf:/Users/smitdave/Zotero/storage/IFIKV99N/Singh et al. - 2003 - Microbial diversity of biofilms in dental unit wat.pdf:application/pdf},
