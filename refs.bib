@article{GethingPW2011ModellingGlobal,
	title = {Modelling the global constraints of temperature on transmission of \textit{{Plasmodium} falciparum} and \textit{{P}. vivax}},
	volume = {4},
	copyright = {All rights reserved},
	issn = {1756-3305},
	url = {http://dx.doi.org/10.1186/1756-3305-4-92},
	doi = {10.1186/1756-3305-4-92},
	abstract = {BACKGROUND: Temperature is a key determinant of environmental suitability for transmission of human malaria, modulating endemicity in some regions and preventing transmission in others. The spatial modelling of malaria endemicity has become increasingly sophisticated and is now central to the global scale planning, implementation, and monitoring of disease control and regional efforts towards elimination, but existing efforts to model the constraints of temperature on the malaria landscape at these scales have been simplistic. Here, we define an analytical framework to model these constraints appropriately at fine spatial and temporal resolutions, providing a detailed dynamic description that can enhance large scale malaria cartography as a decision-support tool in public health. RESULTS: We defined a dynamic biological model that incorporated the principal mechanisms of temperature dependency in the malaria transmission cycle and used it with fine spatial and temporal resolution temperature data to evaluate time-series of temperature suitability for transmission of Plasmodium falciparum and P. vivax throughout an average year, quantified using an index proportional to the basic reproductive number. Time-series were calculated for all 1 km resolution land pixels globally and were summarised to create high-resolution maps for each species delineating those regions where temperature precludes transmission throughout the year. Within suitable zones we mapped for each pixel the number of days in which transmission is possible and an integrated measure of the intensity of suitability across the year. The detailed evaluation of temporal suitability dynamics provided by the model is visualised in a series of accompanying animations. CONCLUSIONS: These modelled products, made available freely in the public domain, can support the refined delineation of populations at risk; enhance endemicity mapping by offering a detailed, dynamic, and biologically driven alternative to the ubiquitous empirical incorporation of raw temperature data in geospatial models; and provide a rich spatial and temporal platform for future biological modelling studies.},
	language = {en},
	journal = {Parasit. Vectors},
	author = {Gething, Peter W and Van Boeckel, Thomas P and Smith, David L and Guerra, Carlos A and Patil, Anand P and Snow, Robert W and Hay, Simon I},
	month = may,
	year = {2011},
	pmid = {21615906},
	keywords = {malaria, mine, seasonality, falciparum, vivax, Bibliographies/Books, Bibliographies/NSF / Microsoft Research, DLS, Spatial, Temperature, Weather, article},
	pages = {92},
	file = {Attachment:/Users/smitdave/Zotero/storage/D7F4CBBX/Gething et al. 2011 - 2E9AB1AC-9CEF-4DCC-B820-79E1E4846258.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/E2XUISR2/Gething et al. 2011 - Modelling the global constraints of temperature on transmission of Plasmodium falciparum and P. vivax.pdf:application/pdf;Gething et al. - 2011 - Modelling the global constraints of temperature on.pdf:/Users/smitdave/Zotero/storage/8Q2JWT3Q/Gething et al. - 2011 - Modelling the global constraints of temperature on.pdf:application/pdf},
}

