	note = {Publisher: Academic Press},
	keywords = {Bibliographies/Books, Dynamic Model, Malaria Models, bib\_from\_DG.ris, Spatial, MBPT model, SystematicReview1970-2010, spatial dynamic model, metapopulation model},
	pages = {31--53},
	file = {Attachment:/Users/smitdave/Zotero/storage/PY4HU9T7/Hasibeder and Dye 1988 - Population dynamics of mosquito-borne disease - persistence in a completely heterogeneous environment.pdf:application/pdf;Hasibeder and Dye - 1988 - Population dynamics of mosquito-borne disease per.pdf:/Users/smitdave/Zotero/storage/QJ7853BG/Hasibeder and Dye - 1988 - Population dynamics of mosquito-borne disease per.pdf:application/pdf},
}

@article{KiwareSS2016GenericSchema,
	title = {A generic schema and data collection forms applicable to diverse entomological studies of mosquitoes},
	volume = {11},
	issn = {1751-0473},
	url = {http://dx.doi.org/10.1186/s13029-016-0050-1},
	doi = {10.1186/s13029-016-0050-1},
	abstract = {BACKGROUND: Standardized schemas, databases, and public data repositories are needed for the studies of malaria vectors that encompass a remarkably diverse array of designs and rapidly generate large data volumes, often in resource-limited tropical settings lacking specialized software or informatics support. RESULTS: Data from the majority of mosquito studies conformed to a generic schema, with data collection forms recording the experimental design, sorting of collections, details of sample pooling or subdivision, and additional observations. Generically applicable forms with standardized attribute definitions enabled rigorous, consistent data and sample management with generic software and minimal expertise. Forms use now includes 20 experiments, 8 projects, and 15 users at 3 research and control institutes in 3 African countries, resulting in 11 peer-reviewed publications. CONCLUSION: We have designed generic data schema that can be used to develop paper or electronic based data collection forms depending on the availability of resources. We have developed paper-based data collection forms that can be used to collect data from majority of entomological studies across multiple study areas using standardized data formats. Data recorded on these forms with standardized formats can be entered and linked with any relational database software. These informatics tools are recommended because they ensure that medical entomologists save time, improve data quality, and data collected and shared across multiple studies is in standardized formats hence increasing research outputs.},
	language = {en},
	number = {1},
	journal = {Source Code Biol. Med.},
	author = {Kiware, Samson S and Russell, Tanya L and Mtema, Zacharia J and Malishee, Alpha D and Chaki, Prosper and Lwetoijera, Dickson and Chanda, Javan and Chinula, Dingani and Majambere, Silas and Gimnig, John E and Smith, Thomas A and Killeen, Gerry F},
	month = mar,
	year = {2016},
	pmid = {27022408},
	note = {Publisher: scfbm.biomedcentral.com},
	keywords = {bib\_from\_DG.ris, Bioinformatics, Computational Biology/Bioinformatics, Computer Appl. in Life Sciences},
	pages = {4},
	file = {Attachment:/Users/smitdave/Zotero/storage/Q8VD2G2Z/Kiware et al. 2016 - A generic schema and data collection forms applicable to diverse entomological studies of mosquitoes.pdf:application/pdf},
}

@article{MacdonaldG1964MalariaParasite,
	title = {The malaria parasite rate and interruption of transmission},
	volume = {31},
	issn = {0042-9686},
	url = {https://www.ncbi.nlm.nih.gov/pubmed/14267746},
	abstract = {Present methods for assessment of the attack phase of malaria eradication are inadequate, particularly lacking any objective parasitological criteria of success. On the basis of previous observations and development of theory, the authors first postulate that the effects of complete interruption of transmission should, ideally, include a regular progressive decrease of falciparum parasite rates in the ratios of 1: 0.4 in 6 months, 1: 0.16 in 12 months and 1: 0.026 in 24 months. Analysis of a series of programmes in which complete interruption of transmission is known to have been achieved shows that this postulate is valid, and that it is not materially upset by strain differences of parasites or by differences in the ages of the subjects examined. Vivax rates appear to fall at approximately the same rate; the rarity of data for vivax malaria makes firm conclusions unsure, but the postulate can be extended to rates which are predominantly due to falciparum infection but include some admixture of other species. A second, arbitrary, postulate is made that the slowest acceptable rate of fall in 12 months should be in a ratio not less than 1: 0.22, which would secure ultimate eradication in about one-third more time than the ideal fall; on this basis statistical standards are set up for assurance of confidence that the minimum rate is exceeded. Slower rates of fall are then related to reproduction rates causing them, the findings being illustrated graphically and by mathematical theory.},
	language = {en},
	journal = {Bull. World Health Organ.},
	author = {Macdonald, G and Goeckel, G W},
	year = {1964},
	pmid = {14267746},
	keywords = {malaria, falciparum, vivax, Bibliographies/Books, Dynamic Model, Malaria Models, bib\_from\_DG.ris, Dynamical Perspective, MOSQUITO CONTROL, Malaria Models/1897-1969, Response Timelines, STATISTICS, TROPICAL MEDICINE, malaria control, George Macdonald, infection duration},
	pages = {365--377},
	file = {Attachment:/Users/smitdave/Zotero/storage/YWIQUWK2/Macdonald and Goeckel 1964 - The malaria parasite rate and interruption of transmission.pdf:application/pdf;Macdonald and Göeckel - 1964 - The malaria parasite rate and interruption of tran.pdf:/Users/smitdave/Zotero/storage/J7Z9ANNE/Macdonald and Göeckel - 1964 - The malaria parasite rate and interruption of tran.pdf:application/pdf},
}

