	note = {Publisher: American Society of Tropical Medicine and Hygiene},
	keywords = {malaria, Uganda, Bibliographies/Books, DLS, Grants/PRISM, Scaling, PRISM, NIAID U19AI089674, power law, Uganda Malaria, article},
