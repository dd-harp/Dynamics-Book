	year = {2010},
	keywords = {malaria, mine, heterogeneous biting, PfEIR, PfFOI, Bibliographies/Books, DLS, EIR, Metrics, FOI, inefficient transmission, power law, MBPT model, SystematicReview1970-2010, PfEIR-PfPR, PfEIR-PfFoI, article},
