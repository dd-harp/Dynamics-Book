	pages = {580--585},
	file = {Attachment:/Users/smitdave/Zotero/storage/FHNME9ZB/Furuno et al. 2006 - 54443906-3C75-4645-9D4D-AA2284B3CC6A.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/JZVGXBZF/Furuno et al. 2006 - Identifying groups at high risk for carriage of antibiotic-resistant bacteria.pdf:application/pdf;Furuno et al. - 2006 - Identifying groups at high risk for carriage of an.pdf:/Users/smitdave/Zotero/storage/UMU8W7WB/Furuno et al. - 2006 - Identifying groups at high risk for carriage of an.pdf:application/pdf},
