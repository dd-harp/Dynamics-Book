	pages = {19},
	file = {Attachment:/Users/smitdave/Zotero/storage/VS3SY7EN/Kelly-Hope and McKenzie 2009 - The multiplicity of malaria transmission ... l inoculation rate measurements and methods across sub-Saharan Africa.pdf:application/pdf},
}

