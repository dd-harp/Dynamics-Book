@article{ElliottRC2018MedicalEntomological,
	title = {Medical and entomological malarial interventions, a comparison and synergy of two control measures using a {Ross}/{Macdonald} model variant and openmalaria simulation},
	volume = {300},
	copyright = {All rights reserved},
	issn = {0025-5564, 1879-3134},
	url = {http://dx.doi.org/10.1016/j.mbs.2018.04.005},
	doi = {10.1016/j.mbs.2018.04.005},
	abstract = {Using an established Ross/Macdonald model variant for mosquito-born parasite transmission, we extend the formalism to simply incorporate time-dependent control measures. In particular, two interventions are considered, mass drug administration (MDA) and indoor residual spraying (IRS), whose individual intensities during their respective campaigns are set to the same intervention-reduced reproductive number R0. The impacts of these interventions, measured as each campaign's ability over time to reduce infections in a community, are found based on the transmission setting, coverage, and their associated durations. These impacts are compared for both interventions and their joint deployment. Synchronous campaigns of IRS deployed with MDA have a cooperative, synergistic effect whose impact exceeds that when the campaigns are deployed in isolation. Simulations with openmalaria, with its more complex model of transmission, are separately performed and show a similar impact enhancement with these interventions. A new, associated analysis yields simple scaling relationships that estimate the dynamical resurgence time, post-intervention, to infection proliferation in a community.},
	language = {en},
	journal = {Math. Biosci.},
	author = {Elliott, R C and Smith, D L and Echodu, D},
	month = jun,
	year = {2018},
	pmid = {29655551},
	keywords = {malaria, Simulation, Indoor residual spraying, Bibliographies/Books, DLS, Dynamic Model, Malaria Models/2016-2020, Models, Mass drug administration, OpenMalaria, Ross/Macdonald, article},
	pages = {187--200},
	file = {Attachment:/Users/smitdave/Zotero/storage/7ISWJJHY/Elliott et al. 2018 - 717B7147-5AC7-4229-8D36-5FF514CF251B.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/YNKU4QDQ/Elliott et al. 2018 - Medical and entomological malarial interventions, ... res using a Ross - Macdonald model variant and openmalaria simulation.pdf:application/pdf},
