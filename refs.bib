	pmid = {12142467},
	note = {Publisher: National Acad Sciences},
	keywords = {falciparum},
	pages = {10689--10694},
	file = {Attachment:/Users/smitdave/Zotero/storage/L2JJZVX4/Peters et al. 2002 - High diversity and rapid changeover of expressed va ... e acute phase of Plasmodium falciparum infections in human volunteers.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/QW2CX89H/Peters et al. 2002 - High diversity and rapid changeover of expressed va ... e acute phase of Plasmodium falciparum infections in human volunteers.pdf:application/pdf},
