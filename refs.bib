	note = {Publisher: eLife Sciences Publications Limited},
	keywords = {malaria, spatial epidemiology, falciparum, microepidemiology, Bibliographies/Books, DLS, Foci, malaria control, hotspot, article},
	pages = {e02130},
	file = {Attachment:/Users/smitdave/Zotero/storage/J8IF7U5C/Bejon et al. 2014 - A micro-epidemiological analysis of febrile malaria in Coastal Kenya showing hotspots within hotspots.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/IYJ5R4DZ/Bejon et al. 2014 - A micro-epidemiological analysis of febrile malaria in Coastal Kenya showing hotspots within hotspots.pdf:application/pdf},
