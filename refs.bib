	pmid = {29718283},
	note = {Publisher: academic.oup.com},
	keywords = {falciparum, Dynamic Model},
	pages = {946--955},
	file = {Attachment:/Users/smitdave/Zotero/storage/UYIQBN6D/Aydemir et al. 2018 - Drug-Resistance and Population Structure of Plasmo ... ic Republic of Congo Using High-Throughput Molecular Inversion Probes.pdf:application/pdf},
