@article{MacdonaldG1956EpidemiologicalBasis,
	title = {Epidemiological basis of malaria control},
	volume = {15},
	issn = {0042-9686},
	url = {https://www.ncbi.nlm.nih.gov/pubmed/13404439},
	abstract = {The epidemiology of malaria is discussed with special reference to the pattern observed in equatorial Africa, where the disease is very stable and where certain features, such as severe epidemic tendencies and ready amenability to control, commonly found in other malarious regions, are lacking. The particular conditions giving rise to stability are described in detail, and the ways in which they can be modified to bring about control of the disease in its stable form are outlined. The importance of measuring certain rates-for example, the basic reproduction rate, the index of stability, and the actual reproduction rate-when making any major malaria survey is emphasized, and formulae by means of which such rates can be readily calculated are included in an annex.},
	language = {en},
	number = {3-5},
	journal = {Bull. World Health Organ.},
	author = {Macdonald, G},
