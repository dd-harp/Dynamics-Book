	keywords = {malaria, mine, oviposition, Bibliographies/Books, Bibliographies/NSF / Microsoft Research, DLS, Cited, transmission, Vector Infection Dynamics, Anchor, bib\_from\_DG.ris, Spatial, mosquito ecology, Egg Laying, malaria dynamics, MBPT model, SystematicReview1970-2010, spatial dynamic model, behavioral state model, mosquito search, mosquito movement, mosquito dispersal, article},
	pages = {23},
