	keywords = {malaria, falciparum, OpenMalaria},
	pages = {56--62},
	file = {Attachment:/Users/smitdave/Zotero/storage/UVHH4QKS/Smith et al. 2006 - An epidemiologic model of the incidence of acute illness in Plasmodium falciparum malaria.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/KZKNJQ2H/Smith et al. 2006 - D38BB5B9-3EDC-44AE-8700-D742198C7272.pdf:application/pdf;Smith et al. - 2006 - AN EPIDEMIOLOGIC MODEL OF THE INCIDENCE OF ACUTE I.pdf:/Users/smitdave/Zotero/storage/LVXQF45S/Smith et al. - 2006 - AN EPIDEMIOLOGIC MODEL OF THE INCIDENCE OF ACUTE I.pdf:application/pdf},
