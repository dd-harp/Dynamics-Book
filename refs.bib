@article{NorthAR2019ModellingPotential,
	title = {Modelling the potential of genetic control of malaria mosquitoes at national scale},
	volume = {17},
	issn = {1741-7007},
	url = {http://dx.doi.org/10.1186/s12915-019-0645-5},
	doi = {10.1186/s12915-019-0645-5},
	abstract = {BACKGROUND: The persistence of malaria in large parts of sub-Saharan Africa has motivated the development of novel tools to complement existing control programmes, including gene-drive technologies to modify mosquito vector populations. Here, we use a stochastic simulation model to explore the potential of using a driving-Y chromosome to suppress vector populations in a 106 km2 area of West Africa including all of Burkina Faso. RESULTS: The consequence of driving-Y introductions is predicted to vary across the landscape, causing elimination of the target species in some regions and suppression in others. We explore how this variation is determined by environmental conditions, mosquito behaviour, and the properties of the gene-drive. Seasonality is particularly important, and we find population elimination is more likely in regions with mild dry seasons whereas suppression is more likely in regions with strong seasonality. CONCLUSIONS: Despite the spatial heterogeneity, we suggest that repeated introductions of modified mosquitoes over a few years into a small fraction of human settlements may be sufficient to substantially reduce the overall number of mosquitoes across the entire geographic area.},
	language = {en},
	number = {1},
	journal = {BMC Biol.},
	author = {North, Ace R and Burt, Austin and Godfray, H Charles J},
	month = mar,
	year = {2019},
	pmid = {30922310},
	keywords = {malaria, mosquito, Driving-Y, Gene-drive},
	pages = {26},
	file = {Attachment:/Users/smitdave/Zotero/storage/YBWGPUVA/North et al. 2019 - Modelling the potential of genetic control of malaria mosquitoes at national scale.pdf:application/pdf},
}

@article{NorthA2013ModellingSpatial,
	title = {Modelling the spatial spread of a homing endonuclease gene in a mosquito population},
	volume = {50},
	issn = {0021-8901},
	url = {http://dx.doi.org/10.1111/1365-2664.12133},
	doi = {10.1111/1365-2664.12133},
	abstract = {SUMMARY: Homing endonuclease genes (HEGs) exist naturally in many single-celled organisms and can show extremely strong genetic drive allowing them to spread through populations into which they are introduced. They are being investigated as tools to manipulate the populations of important vectors of human disease, in particular the mosquitoes that transmit malaria. Before HEGs can be deployed, it is important to study their spatial spread in order to design efficient release strategies.A spatially explicit model is developed to study the spread of a HEG through a landscape whose structure is defined by the distribution of mosquito breeding and feeding sites. The model is motivated by the biology of the major vectors of malaria in Africa. The conditions for spread, fixation and loss of two major types of HEG are explored in different landscapes.In landscapes where mosquito resources are abundant, the conditions for spread are well approximated by a mean-field model. Where a HEG imposes a genetic load, it can cause population extinction, though spatial models more often predict population suppression.In certain types of landscapes where mosquito resources are rare, an introduced HEG may be prevented from moving between local mosquito populations and so a simple release strategy is unlikely to be effective, yet if the HEG succeeds in spreading population extinction is a feasible outcome. Increasing the number of release sites at the expense of releasing fewer mosquitoes per site reduces the probability that a HEG will fail.Synthesis and applications. The model presented asks for the first time how the spatial structure of mosquito populations will influence the effectiveness of a technology that is being rapidly developed for vector control. If homing endonuclease genes (HEGs) are to be used in this way, we have qualified the importance of accounting for landscape characteristics in both the execution and the expectation of their application. The next stage is to use the model to study the spread of HEGs through real landscapes where releases may take place, something that will be facilitated by the results of the present study.},
	language = {en},
	number = {5},
	journal = {J. Appl. Ecol.},
	author = {North, Ace and Burt, Austin and Godfray, H Charles J},
	month = oct,
	year = {2013},
	pmid = {25558082},
	note = {Publisher: Wiley Online Library},
	keywords = {Anopheles, malaria, epidemiology, GM Mosquito, spatial spread, vector-borne disease, spatial modeling, mosquito movement, mosquito dispersal},
	pages = {1216--1225},
	file = {Attachment:/Users/smitdave/Zotero/storage/PBY88DAK/North et al. 2013 - Modelling the spatial spread of a homing endonuclease gene in a mosquito population.pdf:application/pdf},
}

@article{SmithDL2011_Sisyphus,
	title = {Infectious disease. {Solving} the {Sisyphean} problem of malaria in {Zanzibar}},
	volume = {332},
	copyright = {All rights reserved},
	issn = {0036-8075, 1095-9203},
	url = {http://dx.doi.org/10.1126/science.1201398},
	doi = {10.1126/science.1201398},
	abstract = {The Global Malaria Action Plan (GMAP), a consensus framework for coordinated action, aims to end malaria deaths by 2015 and eventually to eradicate malaria ([ 1 ][1]). The plan calls for universal access to effective antimalarial drugs and universal coverage with appropriate vector interventions. Strategic planning for how best to reach these goals has been left to individual countries, some of which have already made plans to eliminate malaria, i.e., to rid their countries of malaria parasites and to suppress transmission from imported malaria (travelers carrying malaria infections from one region into another) so that locally acquired cases are rare ([ 2 ][2]). Critics have argued that plans for national elimination distract attention and resources from the priority of reducing malaria's heavy burden in sub-Saharan Africa ([ 3 ][3]) and that a better strategy would be “control,” i.e., reducing malaria to a minor public health problem. These sides reflect the bipolar history of antimalaria efforts. When funding collapsed for a previous attempt to eradicate malaria, control defined the malaria agenda through decades of neglect. Control and elimination are often presented as opposite sides of a debate over how to allocate billions of dollars allocated globally for malaria aid. But a recent study in Zanzibar ([ 4 ][4]) concluded the dichotomy was false. A more urgent problem is continuity. How can enthusiasm for funding malaria be sustained? Since 2002, Zanzibar has scaled up intervention coverage and sharply reduced malaria ([ 5 ][5], [ 6 ][6]). Zanzibar must now decide how to consolidate success, as other countries that succeed in reducing malaria must also do. To help set an appropriate strategy, Zanzibar commissioned a report to assess the feasibility of malaria elimination ([ 4 ][4]), with the goal of comparing requirements for elimination and control (i.e., sustaining current interventions). Elimination would allow Zanzibar to scale back some interventions and, once malaria was sufficiently rare, to transition to a system of active case detection and outbreak response ([ 4 ][4]). This would require substantial intensification of surveillance and building capacity but increase costs from the current control program by less than \$1 per capita ([ 7 ][7]). If imported malaria cases dropped, costs would also decline, and elimination would then cost less than control. Despite operational differences, long-term implications of both strategies were found to be similar. In both cases, interventions had to be maintained to prevent imported malaria from leading to a resumption of local transmission. In other words, the conclusion was not that control or elimination would be a better strategy; it was that regardless of what Zanzibar chooses to do, malaria will remain a threat for many years. The problem remains: how to continue funding a program to manage the risk of resurgence that will persist until the threat of malaria is gone. Zanzibar has been at these crossroads before. The capital, Stone Town, was called a “hot bed of malaria” ([ 8 ][8]). Zanzibar had reduced malaria to very low levels by 1969 ([ 9 ][9]), but with malaria no longer a visible threat to health, interventions were abandoned in 1970. Maintaining a minimal malaria burden despite the continued presence of mosquitoes and other conditions that make an area receptive to malaria requires active suppression of transmission, so malaria returned to levels seen before control (see the graph, opposite page). In 2002, before scale-up of interventions, malaria transmission in Zanzibar was as intense as anywhere in Africa. Without maintaining programs that minimize the burden today, malaria would return again to Zanzibar. This history recalls the myth of Sisyphus, who was forced by Zeus to push a boulder up a hill only to have it roll back down and therefore suffer an eternity of frustration. Malaria's curse is caused by a common, but pernicious, public health conundrum: the greater the success in suppressing an infectious disease, the lower the perceived threat, and the more challenging it becomes to maintain funding and to sustain control. Zanzibar's success shows what is possible and can help build enthusiasm and support for reducing malaria elsewhere. However, the challenge of sustaining success against an imminent but invisible threat must be solved. Failure in Zanzibar would raise questions about the long-term prospects elsewhere in Africa. Zanzibar's dilemma will soon arise in each region that is newly successful in reducing the burden of malaria with the help of donor funding; the uncertainties in Zanzibar are the uncertainties of our global malaria strategy. Zanzibar's malaria funding is once again threatened. International donors are considering the allocation of limited budgets against the few thousand malaria cases that still occur there. The islands are home to only 1.2 million people out of 678 million Africans at risk of malaria ([ 10 ][10]), most of whom also require malaria interventions under the GMAP. Yet the belief that it is possible to shift malaria funds away from Zanzibar arises from basic misunderstandings. First, intensive interventions must remain in place to suppress transmission, or malaria will rapidly return to its highly endemic and high burden baseline, just as it has in the past (see the graph). By analogy, measles epidemics are inevitable if vaccination coverage declines. Malaria interventions must become standard public health interventions, like vaccines. Second, it makes no sense to stop protecting 1.2 million Zanzibaris now that the program has achieved success. Because the costs of reducing malaria greatly depend on drugs, nets, insecticides, and other fixed-price commodities, budgets are roughly proportional to population size, and moving money from Zanzibar would simply protect an equivalent number of citizens in a more populous country. Reshuffling resources among countries, repeatedly sacrificing hard-earned gains to chase new ones, would indeed curse all of Africa with the Sisyphean malaria cycles seen in Zanzibar. ![Figure][11] A plot of malaria parasite surveys in Zanzibar since 1957. Large regional or national surveys (large symbols) and smaller localized surveys (small symbols) of Unguja (purple), Pemba (green), or the entire archipelago (black). Prevalence estimates reported from various age ranges have been standardized to children aged 2 to 10 years ([ 15 ][12]). The first intensive program was part of the Global Malaria Eradication Program (GMEP). The second intensive program, conducted during the 1990s by the Zanzibar Malaria Control Programme (ZMCP) and the U.S. Agency for International Development (USAID), halved prevalence but did not come close to elimination. No extensive surveys were conducted on Unguja until after the current intensive program began. The time span of the three control programs is less shaded. The current intensive program is sponsored by the ZMCP, the U.S. President's Malaria Initiative (PMI); and The Global Fund to Fight AIDS, Tuberculosis, and Malaria (GFATM). (See SOM for details such as error bars.) Finally, it is a mistake to judge the return on malaria investments in relation to the current burden of malaria. A facile weighting of the millions invested in malaria interventions against a few remaining cases produces the erroneous conclusion that those funds are achieving little. The true value of these investments is the number of deaths and cases that are prevented, not that remain. In Zanzibar each year, benefits include prevention of ∼660,000 malaria cases and 3300 malaria-attributable deaths that occurred before control; at the current average control cost of \$3.9 million per year ([ 11 ][13]), this translates into \$1183 per death averted and \$34.5 per disability-adjusted life year averted, in line with the world's most cost-effective public health interventions {\textbackslash}textbackslash[supporting online material (SOM){\textbackslash}textbackslash] ([ 12 ][14]). In Zanzibar, sustained implementation of malaria interventions that make this possible continues to save lives, and it is as cost-effective to continue programs there as it would be to implement a new program at a cost of \$3.9 million per year to prevent nearly 660,000 cases each year somewhere else. For Africa to replicate the success seen in Zanzibar and to achieve the goals of the GMAP, funding levels must match or exceed the current per-capita spending levels in Zanzibar; this investment amounts to at least \$2.2 billion per year to cover 678 million Africans at risk ([ 10 ][10]). The GMAP estimates that almost \$6 billion per year are required globally, but total commitments currently amount to less than \$2 billion per year ([ 13 ][15]). The crux of the problem is that current funding levels are too low to fulfill the GMAP goal of scaling up coverage levels everywhere it is needed, but malaria interventions cannot be scaled back in places like Zanzibar while the threat of malaria remains. The obvious answer is that more money is needed over the next funding cycles to secure the gains that have been made while extending malaria control to new regions. With the severe resource constraints that currently exist, however, the global malaria community needs to define a coherent and efficient long-term strategy. Zanzibar's elimination feasibility report ([ 4 ][4]) points to one viable path, only if the threat of malaria resurgence diminishes. Without renewed transmission from endemic pockets of malaria within the borders or large numbers of imported infections from abroad, countries could scale back current interventions and adopt a system of surveillance and outbreak response. Zanzibar's costs would also be reduced by suppressing malaria transmission in nearby Kenya and Tanzania to reduce imported malaria ([ 4 ][4]). Reducing malaria creates a benefit for neighbors that did not pay for it. Scaling up interventions in regionally coordinated programs, such as those implemented in Southern Africa and Southeast Asia, could increase malaria security for every country in the region, reinforce national success, reduce overall costs, and minimize risks. These regional control zones could be expanded, with additional funds leapfrogged to new countries to advance the front. Through such a spatially progressive approach ([ 14 ][16]), funds could be shifted from successful countries in a strategic, logical way without incurring enormous risks. Zanzibar's achievement must be sustained and integrated into a coherent long-term continental strategy that shrinks Africa's malaria map ([ 14 ][16]). Implementing these programs will require stable long-term funding commitments from donors to sustain the effort over several decades. Countries also must do their part and eventually reduce their reliance on external funding for their malaria programs through predictable domestic funding mechanisms such as an earmarked tax or trust funds ([ 4 ][4]). Malaria endemic countries are not fated to watch their triumph collapse back into failure. Zanzibar's early success shows what can be achieved in Africa. It has proven it can efficiently utilize international resources to reduce malaria dramatically, but funding must be sustained for the next decade or more until a permanent solution to the malaria problem is found. Funders need only to realize that success is possible and that building on the gains being made today is one of the best deals in global health. [www.sciencemag.org/cgi/content/full/332/6036/1384/DC1][17] 1. [↵][18] Roll Back Malaria Partnership (RBMP), The Global Malaria Action Plan for a Malaria-Free World (RBMP, World Health Organization, Geneva, 2008). 2. [↵][19] 1. J. M. Cohen 2. et al ., Malar. J. 9, 213 (2010). [OpenUrl][20][CrossRef][21][PubMed][22] 3. [↵][23] 1. R. W. Snow, 2. K. Marsh , Lancet 376, 137 (2010). [OpenUrl][24][CrossRef][25][PubMed][26][Web of Scienc