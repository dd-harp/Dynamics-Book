	journal = {Elife},
	author = {Pigott, David M and Golding, Nick and Mylne, Adrian and Huang, Zhi and Henry, Andrew J and Weiss, Daniel J and Brady, Oliver J and Kraemer, Moritz U G and Smith, David L and Moyes, Catherine L and Bhatt, Samir and Gething, Peter W and Horby, Peter W and Bogoch, Isaac I and Brownstein, John S and Mekaru, Sumiko R and Tatem, Andrew J and Khan, Kamran and Hay, Simon I},
