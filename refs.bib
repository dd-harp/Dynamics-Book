	note = {Publisher: Elsevier},
	keywords = {Dynamic Model, mosquito, Malaria Models/2011-2015, Dispersal, Mosquito/Movement/Core, mosquito movement, mosquito dispersal},
	pages = {198--216},
	file = {Attachment:/Users/smitdave/Zotero/storage/GDEL3RYE/Lutambi et al. 2013 - Mathematical modelling of mosquito dispersal in a heterogeneous environment.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/LMBK5IA6/Lutambi et al. 2013 - Mathematical modelling of mosquito dispersal in a heterogeneous environment.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/AUG9UK6Q/Lutambi et al. 2013 - Mathematical modelling of mosquito dispersal in a heterogeneous environment.pdf:application/pdf},
