	note = {Publisher: BioMed Central},
	keywords = {Entomology, Bibliographies/Books, Bibliographies/NSF / Microsoft Research, DLS, Cited, mosquito, bib\_from\_DG.ris, Infectious Diseases, Parasitology, Tropical Medicine, Virology, Dispersal, Data, Veterinary Medicine/Veterinary Science, review, MMRR, memory, mosquito dispersal, article},
	pages = {276},
