	pmcid = {PMC5021229},
	keywords = {malaria, immunity, Uganda, falciparum, Child, Bibliographies/Books, DLS, epidemiology, Grants/PRISM, heterogeneity in transmission, power law, Preschool Female Humans Infant Longitudinal Studies Malaria, Uganda Malaria, memory, article},
	pages = {1072--1080},
	file = {Attachment:/Users/smitdave/Zotero/storage/U3LMU8JD/Rodriguez-Barraquer et al. 2016 - 8975B07F-D8DC-49DE-AE10-68C9273E081B.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/WZ4EAPL6/Rodriguez-Barraquer et al. 2016 - Quantifying Heterogeneous Malaria Exposure and Clinical Protection in a Cohort of Ugandan Children.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/7LB4LNMM/Rodriguez-Barraquer et al. 2016 - Quantifying Heterogeneous Malaria Exposure and Clinical Protection in a Cohort of Ugandan Children.pdf:application/pdf;PubMed Central Full Text PDF:/Users/smitdave/Zotero/storage/FPA6IZ7V/Rodriguez-Barraquer et al. - 2016 - Quantifying Heterogeneous Malaria Exposure and Cli.pdf:application/pdf},
