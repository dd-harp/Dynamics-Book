	journal = {Am. J. Trop. Med. Hyg.},
	author = {Conn, Jan E and Norris, Douglas E and Donnelly, Martin J and Beebe, Nigel W and Burkot, Thomas R and Coulibaly, Mamadou B and Chery, Laura and Eapen, Alex and Keven, John B and Kilama, Maxwell and Kumar, Ashwani and Lindsay, Steve W and Moreno, Marta and Quinones, Martha and Reimer, Lisa J and Russell, Tanya L and Smith, David L and Thomas, Matthew B and Walker, Edward D and Wilson, Mark L and Yan, Guiyun},
	month = sep,
	year = {2015},
	pmid = {26259942},
	note = {Publisher: American Society of Tropical Medicine and Hygiene},
	keywords = {malaria, Bibliographies/Books, DLS, NIAID U19AI089674, article},
	pages = {28--41},
