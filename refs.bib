	note = {Publisher: BioMed Central},
	keywords = {malaria, Bibliographies/Books, DLS, Anchor, Epidemiology, BMGF OPP1110495, Health policy, Operational planning, Risk mapping, article},
	pages = {459},
	file = {Attachment:/Users/smitdave/Zotero/storage/XFL49KJZ/Cohen et al. 2017 - Mapping multiple components of malaria risk for improved targeting of elimination interventions.pdf:application/pdf},
