	keywords = {malaria, mine, Bibliographies/Books, DLS, Dynamic Model, Bibliographies/The Sporozoite Rate, Cited, Core, transmission, Vector Infection Dynamics, MBPT model, SystematicReview1970-2010, vectorial capacity, oldMosy, article},
