	pages = {1128--1138},
	file = {Attachment:/Users/smitdave/Zotero/storage/6D2FNC3Q/Reiner et al. 2018 - F716E9B6-699B-4E89-BFE1-3D4DB9B345FD.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/V5Z8QL9Z/Reiner et al. 2018 - Variation in Childhood Diarrheal Morbidity and Mortality in Africa, 2000-2015.pdf:application/pdf},
