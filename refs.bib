	pmid = {16931810},
	note = {Publisher: researchgate.net},
	keywords = {malaria, falciparum, OpenMalaria, MBPT model, SystematicReview1970-2010, memory},
	pages = {1--10},
	file = {Attachment:/Users/smitdave/Zotero/storage/W6NF58K9/Smith et al. 2006 - Mathematical modeling of the impact of malaria vacci ... ology and natural history of Plasmodium falciparum malaria - Overview.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/KPZ9NLWB/Smith et al. 2006 - Mathematical modeling of the impact of malaria vacci ... ology and natural history of Plasmodium falciparum malaria - Overview.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/KD8S7Z57/Smith et al. 2006 - Mathematical modeling of the impact of malaria vacci ... ology and natural history of Plasmodium falciparum malaria - Overview.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/YB8E87AD/Smith et al. 2006 - Mathematical modeling of the impact of malaria vacci ... ology and natural history of Plasmodium falciparum malaria - Overview.pdf:application/pdf;Attachment:/Users/smitdave/Zotero/storage/KSU9ST2F/Smith et al. 2006 - Mathematical modeling of the impact of malaria vacci ... ology and natural history of Plasmodium falciparum malaria - Overview.pdf:application/pdf},
