	note = {Publisher: eLife Sciences Publications, Ltd},
	keywords = {malaria, anti-disease immunity, anti-parasite immunity, immunity, Uganda, falciparum, Bibliographies/Books, DLS, epidemiology, Grants/PRISM, global health, PRISM, BMGF OPP1110495, power law, Uganda Malaria, Age of Infection, memory, article},
	pages = {e35832},
