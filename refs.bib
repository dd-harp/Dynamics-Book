	note = {Publisher: The American Society of Tropical Medicine and Hygiene},
	keywords = {malaria, bib\_from\_DG.ris},
	pages = {167--172},
	file = {Attachment:/Users/smitdave/Zotero/storage/I9GGFZF7/Mockenhaupt et al. 2004 - Manifestation and outcome of severe malaria in children in northern Ghana.pdf:application/pdf},
