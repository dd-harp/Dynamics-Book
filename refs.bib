	pages = {415--430},
	file = {Attachment:/Users/smitdave/Zotero/storage/LWB9PMG3/Hadeler and Dietz 1983 - NONLINEAR HYPERBOLIC PARTIAL DIFFERENTIAL EQUATIONS FOR THE DYNAMICS OF PARASITE POPULATIONS.pdf:application/pdf},
}

